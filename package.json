{
  "name": "framevuerk",
  "version": "2.4.0",
  "description": "Fast, Responsive, Without Dependencies, Both Direction Support and Configurable UI Framework based on Vue.js.",
  "main": "dist/framevuerk.js",
  "scripts": {
    "lint:js": "eslint ./ --ext .js,.vue",
    "lint:js:fix": "eslint ./ --fix --ext .js,.vue",
    "lint:scss": "stylelint '**/*.{vue,scss}' --syntax scss",
    "lint:scss:fix": "echo \"Error: sorry. We have no scss lint fixer now. Use local editor extension instead.\"",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:fix": "npm run lint:js:fix && npm run lint:scss:fix",
    "test": "npm run lint",
    "clean": "rimraf ./dist",
    "build:lib": "node build --source lib -p",
    "build:docs": "node build --source docs -p",
    "build": "npm run build:lib && npm run build:docs",
    "dev": "node build --source docs --watch",
    "build-to-docs": "node build -p -o ../framevuerk-docs/node_modules/framevuerk/dist",
    "serve-to-docs": "node build -w -o ../framevuerk-docs/node_modules/framevuerk/dist",
    "prepublishOnly": "./prepublish.sh"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.1",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-vue": "^6.0.1",
    "file-loader": "^5.0.2",
    "font-awesome": "^4.7.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "null-loader": "^3.0.0",
    "raw-loader": "^4.0.0",
    "rimraf": "^3.0.0",
    "style-loader": "^1.1.2",
    "vue": "^2.6.11",
    "vue-component-style": "^1.0.2",
    "vue-docgen-api": "^4.2.2",
    "vue-docgen-loader": "^1.3.0",
    "vue-highlightjs": "^1.3.3",
    "vue-loader": "^15.7.2",
    "vue-meta": "^2.3.1",
    "vue-router": "^3.1.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.2",
    "webpack-dev-server": "^3.10.1",
    "webpackbar": "^4.0.0"
  },
  "peerDependencies": {
    "vue": ">= 2.2.0 < 3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/framevuerk/framevuerk.git"
  },
  "keywords": [
    "vue",
    "component",
    "framework",
    "framevuerk",
    "ui",
    "ux",
    "responsive",
    "full responsive",
    "configurable",
    "fast",
    "lightweight",
    "rtl",
    "ltr"
  ],
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=5.0.0"
  },
  "author": "nainemom <nainemom@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/framevuerk/framevuerk/issues"
  },
  "homepage": "https://github.com/framevuerk/framevuerk#readme"
}
